<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <servlet>
        <servlet-name>patientSignup</servlet-name>
        <servlet-class>Action.patientRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>patientSignup</servlet-name>
        <url-pattern>/patientReg</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>doctorSignup</servlet-name>
        <servlet-class>Action.doctorRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>doctorSignup</servlet-name>
        <url-pattern>/doctorReg</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>cloudSignin</servlet-name>
        <servlet-class>Action.cloudLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cloudSignin</servlet-name>
        <url-pattern>/cloud</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>taSignin</servlet-name>
        <servlet-class>Action.taLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>taSignin</servlet-name>
        <url-pattern>/ta</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DataUpload</servlet-name>
        <servlet-class>Action.dataUpload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataUpload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>downloadFile</servlet-name>
        <servlet-class>Action.Download</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>downloadFile</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
